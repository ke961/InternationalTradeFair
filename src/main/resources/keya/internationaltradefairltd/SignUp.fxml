<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="479.0" prefWidth="689.0" style="-fx-background-color: #548581;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="keya.internationaltradefairltd.SignUpController">
   <children>
      <Label alignment="CENTER" layoutX="111.0" layoutY="24.0" prefHeight="55.0" prefWidth="505.0" text="Welcome To  International Trade Fair WebPage" textFill="WHITE">
         <font>
            <Font name="System Bold Italic" size="22.0" />
         </font>
      </Label>
      <Label layoutX="209.0" layoutY="79.0" text="You need to Sign Up first to access in your account" textFill="WHITE">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <Label layoutX="46.0" layoutY="141.0" text="Sign Up Here;" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <TextField fx:id="signUpFirstNameTextField" layoutX="72.0" layoutY="180.0" onKeyTyped="#firstNameOnKeyTyped" prefHeight="26.0" prefWidth="206.0" promptText="First Name" />
      <TextField fx:id="signUpLastNameTextField" layoutX="356.0" layoutY="180.0" prefHeight="26.0" prefWidth="206.0" promptText="Last Name" />
      <TextField fx:id="signUpUserNameTextField" layoutX="72.0" layoutY="231.0" onKeyTyped="#userNameOnKeyTyped" prefHeight="26.0" prefWidth="206.0" promptText="User Name" />
      <TextField fx:id="signUpPhoneNumberTextField" layoutX="356.0" layoutY="231.0" onKeyTyped="#phoneNumberOnKeyTyped" prefHeight="26.0" prefWidth="206.0" promptText="Phone Number" />
      <TextField fx:id="signUpPasswordTextField" layoutX="72.0" layoutY="279.0" onKeyTyped="#passOnKeyTyped" prefHeight="26.0" prefWidth="206.0" promptText="Password" />
      <TextField fx:id="signUpConfirmPasswordTextField" layoutX="356.0" layoutY="279.0" onKeyTyped="#conPassOnKeyTyped" prefHeight="26.0" prefWidth="206.0" promptText="Confirm Password" />
      <Button layoutX="229.0" layoutY="322.0" mnemonicParsing="false" onAction="#creatAccountBTOnAction" prefHeight="26.0" prefWidth="206.0" text="Creat Account" />
      <Label layoutX="229.0" layoutY="361.0" text="Already have an account?" textFill="WHITE" />
      <Button layoutX="364.0" layoutY="357.0" mnemonicParsing="false" onAction="#logInBTOnAction" prefHeight="18.0" prefWidth="48.0" style="-fx-background-color: tansper;" text="Login" textFill="WHITE" underline="true" />
      <Button layoutX="610.0" layoutY="437.0" mnemonicParsing="false" onAction="#signUpbackBTOnAction" prefHeight="26.0" prefWidth="70.0" text="Back" />
   </children>
</AnchorPane>
